</div>
<div id="empresas">
    <button data-company="ienel" onclick="selectCompany('ienel')" disabled>IENEL</button>
    <button data-company="trja" onclick="selectCompany('trja')" disabled>3JA</button>
    <button data-company="enp" onclick="selectCompany('enp')" disabled>ENP</button>
